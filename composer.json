{
  "name": "northrook/debug-dump",
  "description": "Dump and debug PHP variables",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Martin Nielsen",
      "email": "mn@northrook.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "@stable",
    "phpstan/phpstan": " @stable",
    "northrook/php-cs": "dev-main",
    "northrook/logger": "dev-main",
    "symfony/error-handler": "@stable",
    "tracy/tracy": "^2.10"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src"
  }
}
